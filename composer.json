{
	"name": "nielslange/smntcs-google-analytics",
	"description": "Adds Google Analytics tracking code to your site.",
	"homepage": "https://github.com/nielslange/smntcs-google-analytics",
	"type": "wordpress-plugin",
	"keywords": [
		"WordPress",
		"Plugin",
		"SMNTCS Google Analytics"
	],
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "Niels Lange",
			"email": "info@nielslange.de"
		}
	],
	"require": {
		"php": ">=7.0"
	},
	"require-dev": {
		"dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
		"humanmade/psalm-plugin-wordpress": "^2.0",
		"overtrue/phplint": "^2.3",
		"phpcompatibility/phpcompatibility-wp": "^2.1",
		"squizlabs/php_codesniffer": "^3.6",
		"vimeo/psalm": "^4.7",
		"wp-coding-standards/wpcs": "^2.3"
	},
	"scripts": {
		"cbf": "phpcbf --standard=phpcs.dist.xml .",
		"cs": "phpcs --standard=phpcs.dist.xml .",
		"lint": "phplint",
		"psalm": "psalm --no-cache"
	}
}
